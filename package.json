{
  "name": "Scenic",
  "version": "1.1.0",
  "engines": {
    "atom": "*",
    "node": "*",
    "vscode": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/UCSCFormalMethods/Scenic-tmLanguage.git"
  },
  "bugs": {
    "url": "https://github.com/UCSCFormalMethods/Scenic-tmLanguage/issues"
  },
  "description": "Syntax highlighter for the Scenic scenario description language.",
  "license": "MIT",
  "publisher": "Daniel J. Fremont",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "scenic",
    "highlighter",
    "syntax"
  ],
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#003c6c",
    "theme": "dark"
  },
  "contributes": {
    "//": [
      "This section is for VSCode.",
      "NOTE: remember to update grammar/src/Scenic.syntax.yaml, too."
    ],
    "languages": [
      {
        "id": "scenic",
        "aliases": [
          "scenic"
        ],
        "extensions": [
          ".scenic"
        ]
      }
    ],
    "grammars": [
      {
        "language": "scenic",
        "scopeName": "source.scenic",
        "path": "./grammars/Scenic.tmLanguage"
      }
    ]
  },
  "devDependencies": {
    "oniguruma": "^7.0.0",
    "syntaxdev": "^0.1.3"
  },
  "dependencies": {}
}
